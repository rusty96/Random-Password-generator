#!/bin/python3
import random
chars = 'abcdefghijklmnopqrstuvwxyz0123456789'
number = input('number of passwords?')
number = int(number)

length = input('passwordlength?')
length = int(length)

for p in range(number):
    password = ''
    for c in range(length):
        password += random.choice(chars)
    print(password)
